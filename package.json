{
  "name": "tinymce-vue-h",
  "version": "5.9.2",
  "description": "基于vue-cli3对tinymce富文本编辑器的二次封装,实现双向绑定等",
  "private": false,
  "author": "Mr.H",
  "license": "MIT",
  "main": "dist/tinymce-vue-h.umd.min.js",
  "keywords": [
    "tinymce-vue-h",
    "vue",
    "tinymce"
  ],
  "files": [
    "dist/*",
    "src/*"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:MrH-OS/tinymce-vue-h.git"
  },
  "bugs": {
    "url": "https://github.com/MrH-OS/tinymce-vue-h",
    "email": "osorso@163.com"
  },
  "homepage": "https://github.com/MrH-OS/tinymce-vue-h",
  "directories": {
    "dist": "dist"
